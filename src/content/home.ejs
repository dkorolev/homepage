<!doctype html>
<h1><%=name%></h1>
<% if(hasOwnProperty('profiles') && profiles.length) { %>
<ul>
<% profiles.forEach(function(e) { %>
  <% if (e.hasOwnProperty('name') && e.hasOwnProperty('comment') && e.hasOwnProperty('url')) { %>
    <% if (e.url.substr(0, 7) === 'mailto:') { %>
      <li><a href="<%=e.url%>"><%=e.name%></a>: <%=e.comment%></li>
    <% } else { %>
      <li><a href="/r?url=<%=e.url%>"><%=e.name%></a>: <%=e.comment%></li>
    <% } %>
  <% } %>
<% }); %>
</ul>
<% } %>
